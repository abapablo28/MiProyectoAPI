{
  "$schema": "https://github.com/Azure/data-api-builder/releases/download/v1.3.19/dab.draft.schema.json",
  "data-source": {
    "database-type": "mssql",
    "connection-string": "@env('DATABASE_CONNECTION_STRING')",
    "options": {
      "set-session-context": false
    }
  },
  "runtime": {
    "rest": {
      "enabled": true,
      "path": "/rest",
      "request-body-strict": true
    },
    "graphql": {
      "enabled": true,
      "path": "/graphql",
      "allow-introspection": true
    },
    "host": {
      "cors": {
        "origins": [],
        "allow-credentials": false
      },
      "authentication": {
        "provider": "StaticWebApps"
      },
      "mode": "production"
    }
  },
  "entities": {"Monedas": {
  "source": {
    "object": "dbo.Monedas",
    "type": "table",
    "keyFields": ["Id"]
  },
  "permissions": [
    {
      "role": "anonymous",
      "actions": [
        "read",
        "create",
        "update",
        "delete"
      ]
    }
  ],
  "rest": true,
  "restPath": "monedas"
}
}
}